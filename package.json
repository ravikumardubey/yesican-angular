{
  "name": "yesican",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node dist/yesican/server/main.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "tinymce-css": "gulp tinymceStyles",
    "postbuild": "node ng-build.js",
    "dev:ssr": "ng run yesican:serve-ssr",
    "serve:ssr": "node dist/yesican/server/main.js",
    "build:ssr": "ng build --configuration production && ng run yesican:server:production",
    "prerender": "ng run yesican:prerender",
    "aws-deploy": "npm run tinymce-css && serverless deploy && ng build --configuration production &&  aws s3 sync dist/yesican/browser/ s3://yesican-fe",
    "aws-remove-cache": "aws cloudfront create-invalidation --distribution-id EJ5UV3HLGJFSW --paths /* && aws cloudfront create-invalidation --distribution-id E3G3EFWIAPGFKL --paths /*"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.5",
    "@angular/cdk": "^16.2.4",
    "@angular/common": "^16.2.5",
    "@angular/compiler": "^16.2.5",
    "@angular/core": "^16.2.5",
    "@angular/forms": "^16.2.5",
    "@angular/platform-browser": "^16.2.5",
    "@angular/platform-browser-dynamic": "^16.2.5",
    "@angular/platform-server": "^16.2.5",
    "@angular/router": "^16.2.5",
    "@ng-bootstrap/ng-bootstrap": "^15.1.1",
    "@ng-select/ng-select": "^11.1.1",
    "@nguniversal/express-engine": "^16.2.0",
    "@popperjs/core": "^2.11.8",
    "@tinymce/tinymce-angular": "^7.0.0",
    "aws-serverless-express": "^3.4.0",
    "bootstrap": "^5.2.3",
    "dayjs": "^1.11.9",
    "express": "^4.18.2",
    "ngx-cookie-service-ssr": "^16.0.1",
    "ngx-image-cropper": "^7.0.2",
    "rxjs": "~7.8.1",
    "tinymce": "^6.7.0",
    "tslib": "^2.6.2",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.2",
    "@angular/cli": "~16.2.2",
    "@angular/compiler-cli": "^16.2.5",
    "@angular/localize": "^16.2.5",
    "@nguniversal/builders": "^16.2.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~4.3.5",
    "@types/node": "^20.6.1",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-sass": "^5.1.0",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "serverless": "^3.35.2",
    "serverless-api-compression": "^1.0.1",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-associate-waf": "^1.2.1",
    "serverless-offline": "^12.0.4",
    "typescript": "~5.1.6"
  }
}
